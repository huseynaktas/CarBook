@model CreateFeatureDto

@{
    ViewData["Title"] = "CreateCar";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}


<div class="page-content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-xl-12">
                <div class="card">
                    <div class="card-body">

                        <h4 class="card-title">Yeni Özellik Girişi</h4>

                        <form method="post">
                            
                            <div class="form-group">
                                <label for="exampleInputName">Özellik Adı</label>
                                <input type="text" class="form-control" id="exampleInputName" placeholder="Model" asp-for="name">
                            </div>
                            
                            <button type="submit" class="btn btn-primary waves-effect waves-light">Ekle</button>
                        </form>

                    </div> 
                </div>
            </div>
        </div>
    </div>
</div>

